/*!
  \file tws/tws_version.hpp

  \brief TWS version information.

  \author Gilberto Ribeiro de Queiroz
 */

#ifndef __TWS_TWS_VERSION_HPP__
#define __TWS_TWS_VERSION_HPP__

//! Major version of TWS (or version).
#define TWS_VERSION_MAJOR @TWS_VERSION_MAJOR@

//! Minor version of TWS (or revision).
#define TWS_VERSION_MINOR @TWS_VERSION_MINOR@

//! Patched version of TWS (or patch).
#define TWS_VERSION_PATCH @TWS_VERSION_PATCH@

//! Release status: alpha, beta, rc (release candidate), stable.
#define TWS_VERSION_STATUS "@TWS_VERSION_STATUS@"

//! A string description of TWS version.
#define TWS_VERSION_STRING "@TWS_VERSION_STRING@"

//! TWS version.
#define TWS_VERSION @TWS_VERSION@

#endif  // __TWS_TWS_VERSION_HPP__